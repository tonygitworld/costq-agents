{
  "servers": {
    "gcp-cost": {
      "command": "python",
      "args": ["-m", "costq_agents.mcp.gcp_cost_mcp_server.server"],
      "cwd": ".",
      "env": {
        "FASTMCP_LOG_LEVEL": "INFO",
        "PYTHONPATH": "."
      },
      "description": "GCP Cost MCP 服务器 - 提供 get_today_date 等时间工具"
    }
  },
  "plugins": {
    "cost_optimizer": {
      "enabled": true,
      "config": {
        "threshold": 1000,
        "currency": "USD"
      }
    },
    "notification": {
      "enabled": false,
      "config": {
        "email": "admin@example.com",
        "slack_webhook": ""
      }
    }
  }
}
